# Environment Variables

# ChromaDB Connection
CHROMA_DB_HOST=localhost
CHROMA_DB_PORT=8000

# Data Storage Paths
FRONTEND_RAG_DATA_DIR=~/.frontend-rag
CHROMA_DB_DATA=~/.frontend-rag/chroma_data

# Server Settings
AUTO_DETECT=true
DEBUG=false
LOG_LEVEL=info

# Vector DB Settings
COLLECTION_PREFIX=mcp_frontend_
DEFAULT_COLLECTION=mcp_frontend_default

# Project Settings
MAX_SEARCH_RESULTS=5
SIMILARITY_THRESHOLD=0.7
CACHE_TTL=3600
