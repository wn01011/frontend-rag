{
  "name": "mcp-frontend-rag",
  "version": "1.0.1",
  "description": "MCP-based RAG server for frontend development guidelines with ChromaDB",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-frontend-rag": "./dist/index.js"
  },
  "files": [
    "dist",
    "guidelines",
    "projects/default",
    "projects/registry.json",
    "server.js",
    "dashboard.html",
    "docker-compose.yml",
    "setup.sh",
    "start-chromadb.sh",
    "README.md",
    "QUICKSTART.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "index": "tsx src/scripts/indexGuidelines.ts",
    "test:collections": "tsx src/scripts/testCollectionManagement.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "rag",
    "frontend",
    "guidelines",
    "vector-db",
    "chromadb",
    "model-context-protocol",
    "claude",
    "ai-assistant"
  ],
  "author": "wn01011",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wn01011/frontend-rag.git"
  },
  "bugs": {
    "url": "https://github.com/wn01011/frontend-rag/issues"
  },
  "homepage": "https://github.com/wn01011/frontend-rag#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "chromadb": "^1.10.5",
    "chromadb-default-embed": "^2.14.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "uuid": "^11.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
