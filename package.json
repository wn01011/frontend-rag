{
  "name": "mcp-frontend-rag",
  "version": "1.0.0",
  "description": "MCP-based RAG server for frontend development guidelines",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "index": "tsx src/scripts/indexGuidelines.ts",
    "test:collections": "tsx src/scripts/testCollectionManagement.ts"
  },
  "keywords": [
    "mcp",
    "rag",
    "frontend",
    "guidelines",
    "vector-db"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "chromadb": "^1.10.5",
    "chromadb-default-embed": "^2.14.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "uuid": "^11.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
